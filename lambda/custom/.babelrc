{
    "presets": [
        [
            "@babel/typescript",
            {
                "targets": {
                    "node": "8.10"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ],
    "sourceMaps": "inline",
    "retainLines": true
}